{
  "name": "uposatha-app",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run frontend\" \"npm run server\"",
    "frontend": "vite",
    "server": "npm run --prefix server dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint",
    "postinstall": "patch-package",
    "sync-version": "node --no-warnings -e \"require('child_process').execSync('npx capacitor-set-version set:android -v ' + require('./package.json').version + ' -b ' + Math.floor(Date.now() / 1000), {stdio: 'inherit', env: {...process.env, NODE_NO_WARNINGS: '1'}})\""
  },
  "dependencies": {
    "@capacitor/android": "^8.1.0",
    "@capacitor/app": "8.0.0",
    "@capacitor/camera": "^8.0.1",
    "@capacitor/core": "8.1.0",
    "@capacitor/filesystem": "^8.1.2",
    "@capacitor/haptics": "8.0.0",
    "@capacitor/keyboard": "8.0.0",
    "@capacitor/local-notifications": "^8.0.0",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/share": "^8.0.1",
    "@capacitor/status-bar": "8.0.0",
    "@ionic/react": "^8.5.0",
    "@ionic/react-router": "^8.5.0",
    "@ishubhamx/panchangam-js": "^2.2.2",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "astronomy-engine": "^2.1.19",
    "browser-image-compression": "^2.0.2",
    "chinese-lunar-calendar": "^1.0.1",
    "country-state-city": "^3.2.1",
    "ionicons": "^7.4.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "tz-lookup": "^6.1.25"
  },
  "devDependencies": {
    "@capacitor/cli": "8.1.0",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "capacitor-set-version": "^2.2.0",
    "concurrently": "^9.2.1",
    "cypress": "^13.5.0",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^22.1.0",
    "patch-package": "^8.0.1",
    "sharp": "^0.34.5",
    "terser": "^5.4.0",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.24.0",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  },
  "description": "An Ionic project"
}
